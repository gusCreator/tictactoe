<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>

<StackPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="hellofx.InicioController" fx:id="fondo">
    <VBox alignment="CENTER" spacing="50" fx:id="content" VBox.vgrow="ALWAYS">
        <VBox alignment="CENTER" spacing="-40">
            <Label fx:id="title1"  VBox.vgrow="ALWAYS"/>
            <Label fx:id="title2" VBox.vgrow="ALWAYS"/>
        </VBox>
        <VBox VBox.vgrow="ALWAYS">
            <HBox alignment="CENTER" spacing="20">
                <Button fx:id="conf" onAction="#cConf" HBox.hgrow="ALWAYS"/>
                <Button fx:id="inicio" text="Iniciar" onAction="#cInicio" HBox.hgrow="ALWAYS">
                </Button>
            </HBox>
        </VBox>
        <Label fx:id="bienvenido" VBox.vgrow="ALWAYS"/>
    </VBox>
</StackPane>
