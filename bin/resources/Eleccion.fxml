<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?> 
<?import javafx.scene.layout.HBox?> 

<StackPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="hellofx.EleccionController" fx:id="fondo">
  <VBox alignment="CENTER" spacing="40">
    <Label fx:id="textElection"/>
    <HBox alignment="CENTER" spacing="30">
      <Button fx:id="circle" onAction="#cCircle"/>
      <Button fx:id="equis" onAction="#cEquis"/>
    </HBox>
  </VBox>
</StackPane>